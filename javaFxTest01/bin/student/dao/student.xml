<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="student">
	
	<typeAlias alias="stdVo" type="student.vo.StudentVO"/>
	
	<!-- insert -->
	<insert id="insertStudent" parameterClass="stdVo">
		insert into student (std_num, std_name, std_kor, std_eng, std_mat)
		values ( std_seq.nextVal, #std_name#, #std_kor#, #std_eng#, #std_mat# )
	</insert>

	<!-- 전체학생 정보 가져오기 -->
	<select id="getAllStudent" resultClass="stdVo">
		select * from student
	</select>

</sqlMap>










