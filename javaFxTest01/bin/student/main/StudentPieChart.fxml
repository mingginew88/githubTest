<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="350.0" prefWidth="350.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="student.main.StudentPieChartController">
   <center>
      <PieChart fx:id="stdPieChart" BorderPane.alignment="CENTER" />
   </center>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
   <bottom>
      <Button fx:id="btnClose" mnemonicParsing="false" onAction="#btnCloseClick" text="닫기" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
