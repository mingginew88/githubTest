<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<!-- 
	namespace속성 : 아래에서 작성한 SQL문을 호출할 때 같이 사용된다. 
 -->	
<sqlMap namespace="zipSearch">
	<!-- 
	이 문서에서만 사용될 alias를 지정할 수 있다.
	<typeAlias .... />
	 -->
	<typeAlias alias="zipVo" type="zipsearch.vo.ZipDataVO"/>

	<select id="getSearchDong" parameterClass="String" 
			resultClass="zipVo">
		select * from ziptb
		where dong like '%' || #dong# || '%'
	</select>
	
	<select id="getSearchZipcode" parameterClass="string"
			resultClass="zipVo">
		select * from ziptb
		where zipcode like '%' || #zipcode# || '%'
	</select>
	


</sqlMap>










